graph = JanusGraphFactory.build().set('storage.backend', 'inmemory').set('schema.default', 'none').open()
mgmt = graph.openManagement()
mgmt.makePropertyKey('id').dataType(String.class).make()
mgmt.commit()
graph.tx().rollback()
mgmt = graph.openManagement()
idKey = mgmt.getPropertyKey('id')
mgmt.buildIndex('byIdComposite', Vertex.class).addKey(idKey).buildCompositeIndex()
mgmt.commit()
mgmt.awaitGraphIndexStatus(graph, 'byIdComposite').call()